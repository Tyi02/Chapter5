package com.CSCI185;

public class SomeCoolStuff {
    public static void main(String[] args) {
        double dollabills = 1000.6321321;
        int money = 10;
        String word = "LOL";


        System.out.printf("I have $%1.2f%n", dollabills);
        System.out.printf("%s%n",word);
        System.out.printf("I have $%d%n", money);


        System.out.println();
        System.out.println("Hello \n \n \n Goodbye \t sup");
        System.out.println("""
                Hello
                
                
                Goodbye
                
                    Sup
                """);
          /* The 2 in 1.2 means round to 2 decimal points, the 1 means to put at least 1 digit in front of the decimal.
        f means float for decimal
        /n can space out the printing to different lines
        triple quotes button also works^
        */
        var x = 5;
        var y = 3.2;
        var name = "George";
        /* var means variable, figures it out basically

         */



    }
}
