package com.CSCI185;

public class RectangleDemo2
{
    public static void main(String[] args) {
        Rectangle r1 = new Rectangle();
        r1.setLength(25);
        r1.setWidth(4);

        Rectangle r2 = r1;

        r2.setLength(100);
        r2.setWidth(70);

        r1.writeOutput();
        r2.writeOutput();
    }
}
